<view class="container">
  <loading hidden="{{loadingHide}}">
    {{loadingText}}
  </loading>

  <view class="title">地区详情</view>
  <view class="area_detail_view"> 
      <view class="viewItem">
        <view class="label">地区id：</view>
        <view class="content">
          <view>{{areaId}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">地区名称：</view>
        <view class="content"> 
          <view>{{areanName}}</view>
        </view>
      </view>

      <view>
        <navigator class="btnReturn" open-type="navigateBack" hover-class="navigator-hover">返回</navigator>
      </view>

  </view>
</view>
