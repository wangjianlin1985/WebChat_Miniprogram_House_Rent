<view class="container">
  <loading hidden="{{loadingHide}}">
    {{loadingText}}
  </loading>

  <view class="title">评论详情</view>
  <view class="comment_detail_view"> 
      <view class="viewItem">
        <view class="label">评论id：</view>
        <view class="content">
          <view>{{commentId}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">被评民宿：</view>
        <view class="content"> 
          <view>{{minsuObj}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">评论内容：</view>
        <view class="content"> 
          <view>{{content}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">评论用户：</view>
        <view class="content"> 
          <view>{{userObj}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">评论时间：</view>
        <view class="content"> 
          <view>{{commentTime}}</view>
        </view>
      </view>

      <view>
        <navigator class="btnReturn" open-type="navigateBack" hover-class="navigator-hover">返回</navigator>
      </view>

  </view>
</view>
